Sub Inivar()
Set Oena
Reset Led1
Print #1 , "************ DRIVER matriz LEDs P10(1R)-V701C ************"
Print #1 , Version(1)
Print #1 , Version(3)
Print #1 , "Nummatriz=" ; Nummatriz
Print #1 , "Longbuf=" ; Longbuf
Print #1 , "Longdat=" ; Longdat
Print #1 , "Longdat_mas_uno=" ; Longdat_mas_uno
Print #1 , "Longbuf_mas_uno=" ; Longbuf_mas_uno
Print #1 , "Numtxser=" ; Numtxser

For Tmpb = 1 To Longbuf
   Buffram(tmpb) = 0
Next
Estado_led = 1
For Tmpb = 1 To 2
   Tmpb2 = Tmpb - 1
   Call Gendig(TMPB , Tmpb)
Next

Enable Timer0
Entra_ant = 255
Sale_ant = 255




End Sub


'*******************************************************************************
' Procesamiento de comandos
'*******************************************************************************
Sub Procser()
   Disable Urxc
   'Print #1 , "$" ; Serproc
   Tmpstr52 = Mid(serproc , 1 , 6)
   Numpar = Split(serproc , Cmdsplit(1) , ",")
   If Numpar > 0 Then
      For Tmpb = 1 To Numpar
         Print #1 , Tmpb ; ":" ; Cmdsplit(tmpb)
      Next
   End If


   If Len(cmdsplit(1)) = 6 Then
      Cmdtmp = Cmdsplit(1)
      Cmdtmp = Ucase(cmdtmp)
      Cmderr = 255
      Select Case Cmdtmp
         Case "LEEVFW"
            Cmderr = 0
            Atsnd = "Version FW: Fecha <"
            Tmpstr52 = Version(1)
            Atsnd = Atsnd + Tmpstr52 + ">, Archivo <"
            Tmpstr52 = Version(3)
            Atsnd = Atsnd + Tmpstr52 + ">"


         Case "SETLED"
            If Numpar = 2 Then
               Tmpb = Val(cmdsplit(2))
               If Tmpb < 17 Then
                  Cmderr = 0
                  Atsnd = "Se configura setled a " + Str(tmpb)
                  Estado_led = Tmpb

               Else
                  Cmderr = 5
               End If

            Else
               Cmderr = 4

            End If

         Case "SETBUF"
            If Numpar = 3 Then
               Cmderr = 0
               Tmpw = Val(cmdsplit(2))
               If Tmpw < Longbuf_mas_uno Then
                  Cmderr = 0
                  Tmpb = Hexval(cmdsplit(3))
                  Buffram(tmpw) = Tmpb
                  Atsnd = "Buffram(" + Str(tmpw) + ") = &H " + Hex(tmpb)
               Else
                  Cmderr = 5
               End If
            Else
               Cmderr = 4
            End If

         Case "LEEBUF"
            If Numpar = 2 Then
               Cmderr = 0
               Tmpw = Val(cmdsplit(2))
               If Tmpw < Longbuf_mas_uno Then
                  Cmderr = 0
                  Tmpb = Buffram(tmpw)
                  Atsnd = "Buffram(" + Str(tmpw) + ") = &H " + Hex(tmpb)
               Else
                  Cmderr = 5
               End If
            Else
               Cmderr = 4
            End If


         Case "SETCER"
            Cmderr = 0
            Atsnd = "Encerar buffer"
            For Tmpw4 = 1 To Longbuf
               'Incr Tmpw3
               'Call Disp_buffer(tmpw4 , Tmpw3)
               Buffram(tmpw4) = 0
            Next

         Case "SETDIG"
            If Numpar = 3 Then
               Cmderr = 0
               Tmpb = Val(cmdsplit(2))
               Tmpb2 = Val(cmdsplit(3))
               Atsnd = "Test Digito " + Str(tmpb)
               Call Gendig(tmpb , Tmpb2)
            Else
               Cmderr = 5
            End If

         Case "SETTAB"
            If Numpar = 5 Then
            'Print #1 , "s>"' ; Serproc
               Tmpb = Val(cmdsplit(2))
               Tmpb2 = Val(cmdsplit(3))
               Cmderr = 0
               'Atsnd = "b"
                  If Tmpb < 100 Then
                     Tmpb2 = Val(cmdsplit(3))
                     If Tmpb2 < 100 Then

                        If Cmdsplit(4) = "1" Then
                           If Len(cmdsplit(2)) = 1 Then
                              Call Gendig(10 , 1)
                              Tmpb3 = Val(cmdsplit(2))
                              Call Gendig(tmpb3 , 2)
                           Else
                              Tmpstr = Mid(cmdsplit(2) , 1 , 1)
                              Tmpb3 = Val(tmpstr)
                              Call Gendig(tmpb3 , 1)
                              Tmpstr = Mid(cmdsplit(2) , 2 , 1)
                              Tmpb3 = Val(tmpstr)
                              Call Gendig(tmpb3 , 2)
                           End If
                        Else
                          Call Gendig(10 , 1)
                          Call Gendig(10 , 2)
                        End If

                        If Cmdsplit(5) = "1" Then
                           If Len(cmdsplit(3)) = 1 Then
                              Call Gendig(10 , 3)
                              Tmpb3 = Val(cmdsplit(3))
                              Call Gendig(tmpb3 , 4)
                           Else
                              Tmpstr = Mid(cmdsplit(3) , 1 , 1)
                              Tmpb3 = Val(tmpstr)
                              Call Gendig(tmpb3 , 3)
                              Tmpstr = Mid(cmdsplit(3) , 2 , 1)
                              Tmpb3 = Val(tmpstr)
                              Call Gendig(tmpb3 , 4)
                           End If
                        Else
                          Call Gendig(10 , 3)
                          Call Gendig(10 , 4)
                        End If

                     Else
                        Cmderr = 6

                     End If
                  Else
                     Cmderr = 5
                  End If
            Else
              Cmderr = 10
            End If

         Case Else
            Cmderr = 1

      End Select

   Else
        Cmderr = 2
   End If

   If Cmderr > 0 Then
      Atsnd = Lookupstr(cmderr , Tbl_err)
   End If

   Print #1 , Atsnd

   Enable Urxc
End Sub


'*******************************************************************************
'  Subrutina para pintar digito en posicion predeterminada
'  Los digitos son de 16x16 pixels y se puden posicionar en 4 posiciones
'  Valdig indica el valor del digito (1-9) y posdig la posicion (1-4)
'
Sub Gendig(byval Valdig As Byte , Byval Posdig As Byte)
   Local Kt As Byte
   Local Ptr As Byte
   Local Tmpb22 As Byte
   Ptrdig = Valdig * 32
   Posdig = Posdig - 1
   Ptrpos = Posdig * 32

   For Kt = 1 To 32
      K = Kt - 1
      Tmpw3 = K + Ptrpos
      Ptr = Lookup(tmpw3 , Tbl_posdig)
      Tmpw2 = Ptrdig + K
      Tmpb22 = Lookup(tmpw2 , Tbl_dig)
      Buffram(ptr) = Tmpb22
      'Print #1 , "B(" ; Tmpb ; ")=" ; Bin(ptr)
   Next

End Sub


